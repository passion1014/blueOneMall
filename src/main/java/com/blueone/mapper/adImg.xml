	<select id="selectDtlBomAdImgTb0001" resultType="com.blueone.admin.domain.AdImgInfo" resultMap="AdImgInfoMap">
		SELECT 
			`MD_IMG_1`,`MD_URL_1`,`MD_IMG_2`,`MD_URL_2`,`MD_IMG_3`,`MD_URL_3`,`MD_IMG_4`,
			`MD_URL_4`,`MD_IMG_5`,`MD_URL_5`,`MD_IMG_6`,`MD_URL_6`,`BN_IMG_1`,`BN_URL_1`,`BN_IMG_2`,`BN_URL_2`,
			`BN_IMG_3`,`BN_URL_3`,`BN_IMG_4`,`BN_URL_4`,`BN_IMG_5`,`BN_URL_5`,`BN_IMG_6`,
