<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="admin">

	<insert id="insertBomAdminTb0001" parameterType="com.blueone.admin.domain.AdminInfo">
		INSERT BOM_ADMIN_TB
			(`admin_status`,`admin_id`,`admin_pass`,`admin_name`
				,`admin_phone`,`admin_mobile`,`admin_email`,`admin_grade`
				,`admin_hit`,`admin_cmt`,`regDate`,`lastDate`)
			VALUES
			(#{status}, #{id}, #{password}, #{name}, #{phone}, #{mobile}
				, #{email}, #{grade}, #{hit}, #{comment}, sysdate(), sysdate())
	</insert>

</mapper>
